package com.java.practises;

import java.util.Arrays;
import java.util.List;
import java.util.stream.Collectors;
import java.util.stream.Stream;

public class Demo2 {

	public static void main(String[] args) {
		List<Integer> list = Arrays.asList(12, 23, 43, 44, 567, 789);
		List<Integer> list2 = Arrays.asList(3, 2, 5, 6, 7, 8);
		Stream<Integer> st =  Stream.concat((Stream) list, (Stream) list2);

		List filter = list.stream().filter(e -> Integer.toString(e).startsWith("7")).collect((Collectors.toList()));

		System.out.println(filter);
	}

}
