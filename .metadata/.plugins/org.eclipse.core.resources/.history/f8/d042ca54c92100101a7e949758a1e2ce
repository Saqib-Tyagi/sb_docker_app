package com.java.practises;

import java.util.Arrays;
import java.util.List;
import java.util.stream.Collectors;
import java.util.stream.Stream;

public class Demo2 {

	public static void main(String[] args) {
		List<Integer> list = Arrays.asList(12, 23, 43, 44, 567, 789);
		List<Integer> list2 = Arrays.asList(3, 2, 5, 6, 7, 8);
		Stream<Integer> a1 = list.stream();
		Stream<Integer> a2 = list2.stream();
		Stream<Integer> st = Stream.concat(a1, a2);
		st.collect(Collectors.toList()).forEach(System.out::println);

//		List filter = list.stream().filter(e -> Integer.toString(e).startsWith("7")).collect((Collectors.toList()));

//		System.out.println(filter);
	}

}
